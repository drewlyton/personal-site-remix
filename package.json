{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"npm run build:css:*\" && remix build",
    "build:css:tailwind": "tailwindcss -m -i ./config/styles/main.css -o app/styles/app.css",
    "build:css:scss": "node-sass ./config/styles -o ./config/styles --output-style compressed",
    "dev": "concurrently \"npm run dev:css:*\" \"remix dev\"",
    "dev:css:tailwind": "tailwindcss -w -i ./config/styles/main.css -o app/styles/app.css",
    "dev:css:scss": "npm run build:css:scss && node-sass -rw ./config/styles -o ./config/styles",
    "email": "email dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc -b",
    "start": "cross-env NODE_ENV=production netlify dev"
  },
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "@react-email/button": "0.0.10",
    "@react-email/components": "^0.0.7",
    "@react-email/head": "^0.0.5",
    "@react-email/html": "0.0.4",
    "@react-email/markdown": "^0.0.2",
    "@react-email/render": "^0.0.7",
    "@react-email/tailwind": "0.0.8",
    "@react-email/text": "0.0.5",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/netlify": "^1.7.0",
    "@remix-run/node": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@sanity/client": "^6.11.1",
    "@sanity/image-url": "^1.0.2",
    "@sendgrid/client": "^7.7.0",
    "@sendgrid/mail": "^7.7.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.2",
    "classnames": "^2.3.1",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.11",
    "feed": "^4.2.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "gsap": "^3.9.1",
    "mdx-bundler": "^10.0.1",
    "node-sass": "^7.0.3",
    "postcss": "^8.4.8",
    "prismjs": "^1.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "1.9.4",
    "react-transition-group": "^4.4.2",
    "rehype-katex": "^6.0.2",
    "rehype-mathjax": "^4.0.2",
    "rehype-prism-plus": "^1.4.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^1.7.0",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-transition-group": "^4.4.4",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
